{
    "name": "@tspro/ts-utils-lib",
    "version": "1.16.0",
    "author": "PahkaSoft",
    "license": "MIT",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/pahkasoft/ts-utils-lib.git"
    },
    "bugs": {
        "url": "https://github.com/pahkasoft/ts-utils-lib/issues/new/choose"
    },
    "description": "Useful library of typescript classes, modules and functions.",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "exports": {
        ".": {
            "browser": {
                "require": {
                    "types": "./dist/index.d.ts",
                    "default": "./dist/index.js"
                },
                "import": {
                    "types": "./dist/index.d.mts",
                    "default": "./dist/index.mjs"
                }
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            }
        }
    },
    "files": [
        "CHANGELOG.md",
        "LICENSE",
        "README.md",
        "dist/**/*.js",
        "dist/**/*.mjs",
        "dist/**/*.d.ts",
        "dist/**/*.d.mts",
        "package.json"
    ],
    "devDependencies": {
        "@types/jasmine": "^5.1.8",
        "benchmark": "^2.1.4",
        "jasmine": "^5.8.0",
        "karma": "^6.4.4",
        "karma-chrome-launcher": "^3.2.0",
        "karma-cli": "^2.0.0",
        "karma-jasmine": "^5.1.0",
        "karma-webpack": "^5.0.1",
        "npm-watch": "^0.13.0",
        "ts-loader": "^9.5.2",
        "tsup": "^8.5.0",
        "typedoc": "^0.28.7",
        "typescript": "latest",
        "webpack": "^5.100.0"
    },
    "scripts": {
        "build": "tsup",
        "watch": "tsup --watch",
        "test": "karma start",
        "docs": "typedoc"
    }
}
